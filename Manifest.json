{
  "$schema": "https://qooxdoo.org/schema/Manifest-2-0-0.json",
  "info": {
    "name": "ville.Embed",
    "summary": "A new desktop ui control for the Qooxdoo JavaScript Framework",
    "description": "",
    "homepage": "",
    "license": "MIT license",
    "authors": [
      {
        "name": "Chris Eskew",
        "email": "(sqville)"
      }
    ],
    "version": "4.0.0"
  },
  "provides": {
    "namespace": "ville.embed",
    "encoding": "utf-8",
    "class": "source/class",
    "resource": "source/resource",
    "translation": "source/translation",
    "fonts": {
