{
  "$schema": "https://qooxdoo.org/schema/compile-1-0-0.json",
  "targets": [
    {
      "type": "source",
      "outputPath": "compiled/source",
      "bundle": {
        "include": "qx.*"
      }
    },
    {
      "type": "build",
      "outputPath": "compiled/build",
      "inline-external-scripts": true
    }
  ],
  "defaultTarget": "source",
  "locales": ["en"],

  "applications": [
    {
      "class": "wax.demo.ApplicationTest",
      "theme": "wax.demo.theme.Theme",
      "name": "waxdemo",
      "title": "WaxDemo"
    }
  ]
}
