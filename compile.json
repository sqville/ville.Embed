{
  "$schema": "https://qooxdoo.org/schema/compile-1-0-0.json",
  "targets": [
    {
      "type": "source",
      "outputPath": "compiled/source",
      "typescript": false,
      "bundle": {
        "include": "qx.*"
      }
    },
    {
      "type": "build",
      "outputPath": "compiled/build"
    }
  ],
  "defaultTarget": "source",
  "locales": ["en"],

  "applications": [
    {
      "class": "wax.demo.Application",
      "theme": "wax.demo.theme.Theme",
      "name": "waxdemo",
      "title": "WaxDemo",
      "bootPath": "source/boot"
    }
  ]
}
